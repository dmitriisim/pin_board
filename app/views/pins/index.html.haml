#pins.transitions-enabled
	- @pins.each do |pin|
		.box.panel.panel-default
			= link_to (image_tag pin.image.url), pin
			.panel-body
				%h2= link_to pin.title, pin
				%p.user
				Submitted by:
				= pin.user.email
.text-center
	=will_paginate @posts, renderer: BootstrapPagination::Rails, inner_window: 1, outer_window: 0				
			